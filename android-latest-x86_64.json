{
    "emulator_package": "system-images;android-34;google_apis;x86_64",
    "emulator_avd_name": "mozemulator-android-latest-x86_64",
    "emulator_extra_args": [
        "-skip-adb-auth",
        "-selinux", "permissive",
        "-memory", "3072",
        "-cores", "4",
        "-skin", "800x1280",
        "-gpu", "host",
        "-no-snapstorage",
        "-no-snapshot",
        "-no-window",
        "-prop", "ro.test_harness=true"
    ],
    "emulator_extra_config": {
        "hw.keyboard": "yes",
        "hw.lcd.density": "320",
        "disk.dataPartition.size": "4000MB",
        "sdcard.size": "600M"
    }
}
